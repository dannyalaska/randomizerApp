<!DOCTYPE html>
<html>
<head>
  <title>Randomizer</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if logged_in? %>
  <div id="logout">
    <%= button_to 'Logout', logout_path, :method => :delete %>
  </div>
  <% end %>
  <%= yield %>
</body>
</html>
